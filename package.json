{
  "name": "async-call-rpc",
  "packageManager": "pnpm@7.9.0",
  "version": "6.1.5",
  "description": "A lightweight JSON RPC server & client",
  "main": "out/base.js",
  "module": "out/base.mjs",
  "types": "out/base.d.ts",
  "exports": {
    ".": {
      "types": "./out/base.d.ts",
      "require": "./out/base.js",
      "import": "./out/base.mjs"
    },
    "./full": {
      "types": "./out/full.d.ts",
      "require": "./out/full.js",
      "import": "./out/full.mjs"
    },
    "./full.min": {
      "types": "./out/full.d.ts",
      "require": "./out/full.min.js",
      "import": "./out/full.min.mjs"
    },
    "./base": {
      "types": "./out/base.d.ts",
      "require": "./out/base.js",
      "import": "./out/base.mjs"
    },
    "./base.min": {
      "types": "./out/base.d.ts",
      "require": "./out/base.min.js",
      "import": "./out/base.min.mjs"
    },
    "./utils/*": "./utils/*"
  },
  "typesVersions": {
    "*": {
      "base": [
        "./out/base.d.ts"
      ],
      "base.min": [
        "./out/base.d.ts"
      ],
      "full": [
        "./out/full.d.ts"
      ],
      "full.min": [
        "./out/full.d.ts"
      ]
    }
  },
  "scripts": {
    "publish": "pnpm run clean && pnpm run build && pnpm run doc:api && pnpm test && npx changeset publish",
    "clean": "rimraf ./out ./utils/web ./utils/node ./es ./coverage ./dist",
    "watch:tsc": "tsc -b -w",
    "build:tsc": "tsc -b",
    "watch:rollup": "rollup -c -w",
    "build:rollup": "rollup -c",
    "build": "pnpm run build:tsc && pnpm run build:rollup",
    "doc:api": "api-extractor run --local --verbose && api-extractor run --local -c ./api-extractor-base.json",
    "doc:md": "api-documenter markdown -o docs -i dist/api-extractor",
    "doc": "run-s doc:api doc:md",
    "start": "run-p watch:rollup watch:tsc watch:test",
    "test": "vitest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jack-Works/async-call.git"
  },
  "keywords": [
    "jsonrpc"
  ],
  "author": "Jack Works <zjwpeter@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jack-Works/async-call/issues"
  },
  "homepage": "https://github.com/Jack-Works/async-call#readme",
  "devDependencies": {
    "@changesets/cli": "^2.25.0",
    "@microsoft/api-documenter": "^7.19.22",
    "@microsoft/api-extractor": "^7.33.4",
    "@msgpack/msgpack": "^2.8.0",
    "@swc/core": "^1.3.9",
    "@types/node": "^18.11.0",
    "@types/ws": "^8.5.3",
    "@vitest/coverage-c8": "^0.24.3",
    "@vitest/ui": "^0.24.3",
    "async-call-rpc": "link:",
    "bson": "^4.7.0",
    "c8": "^7.12.0",
    "jest-file-snapshot": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "pretty-format": "^29.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.2.3",
    "rollup-plugin-swc3": "^0.6.0",
    "typescript": "^4.9.1-beta",
    "vite": "^3.1.8",
    "vitest": "^0.24.3",
    "ws": "^8.9.0"
  },
  "files": [
    "src",
    "out",
    "utils",
    "utils-src",
    "!tsconfig.json",
    "!src/package.json"
  ]
}
